<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
	<channel>
		<title>CompliSpace Developer Blog</title>
		<description>Behind the curtain at CompliSpace</description>
		<link>http://complispace.github.com/</link>
		{% for post in site.posts %}
			<item>
				<title>{{ post.title }}</title>
				<description>{{ post.content | xml_escape }}</description>
				<link>http://complispace.github.com{{ post.url }}</link>
			</item>
		{% endfor %}
	</channel>
</rss>